{
    "name": "EnviroDIY_ModularSensors",
    "description":  "A library that allows acces to multiple sensors through a unified interface.  This allows the user to simply access many sensors to log the data or send the data to data repositories like the EnviroDIY data portal.",
    "keywords": "sensors, logger, low power, sleeping, EnviroDIY",
    "platforms": "atmelavr",
    "frameworks": "arduino",
    "authors":
    [
        {
            "name": "Sara Damiano",
            "email": "sdamiano@stroudcenter.org",
            "url": "https://envirodiy.org/",
            "maintainer": true
        },
        {
            "name": "Shannon Hicks",
            "email": "shicks@stroudcenter.org",
            "url": "https://envirodiy.org/"
        }
    ],
    "version": "0.4.1-beta",
    "repository":
    {
        "type": "git",
        "url": "https://github.com/EnviroDIY/ModularSensors.git"
    },
    "export":
    {
        "exclude":
        [
            "doc/*",
            "sensor_tests/*"
        ]
    },
    "dependencies":
    [
        {
            "name": "Sodaq_DS3231",
            "version": "https://github.com/EnviroDIY/Sodaq_DS3231"
        },
        {
            "library id": "311",
            "name": "EnableInterrupt",
            "note": "GreyGnome's EnableInterrupt",
            "url": "https://github.com/GreyGnome/EnableInterrupt"
        },
        {
            "library id": "322",
            "name": "SdFat",
            "url": "https://github.com/greiman/SdFat"
        },
        {
            "library id": "342",
            "name": "ADS1X15",
            "note": "TI ADS1X15 ADC Library by Adafruit",
            "url": "http://github.com/Adafruit/Adafruit_ADS1X15"
        },
        {
            "library id": "31",
            "name": "Adafruit Unified Sensor",
            "note": "Adafruit Unified Sensor Library",
            "url": "https://github.com/adafruit/Adafruit_Sensor"
        },
        {
            "library id": "19",
            "name": "DHT sensor library",
            "note": "AOSong DHT Sensor Library by Adafruit",
            "url": "http://github.com/adafruit/DHT-sensor-library"
        },
        {
            "library id": "773",
            "name": "Adafruit AM2315",
            "note": "AOSong AM2315 I2C Temp/Humidity Sensor Library by Adafruit",
            "url": "https://github.com/adafruit/Adafruit_AM2315"
        },
        {
            "library id": "166",
            "name": "Adafruit BME280 Library",
            "note": "Bosch BME280 Temp/Humidity/Pressure Sensor Library by Adafruit",
            "url": "https://github.com/adafruit/Adafruit_BME280_Library"
        },
        {
            "library id": "1",
            "name": "OneWire",
            "url": "https://github.com/PaulStoffregen/OneWire.git"
        },
        {
            "library id": "54",
            "name": "DallasTemperature",
            "url": "https://github.com/milesburton/Arduino-Temperature-Control-Library.git"
        },
        {
            "name": "Arduino-SDI-12_ExtInts",
            "version": "https://github.com/EnviroDIY/Arduino-SDI-12.git#ExtInts"
        },
        {
            "name": "TinyGSM",
            "version": "https://github.com/EnviroDIY/TinyGSM.git"
        },
        {
            "library id": "1824",
            "name": "SensorModbusMaster",
            "url": "https://github.com/EnviroDIY/SensorModbusMaster.git"
        },
        {
            "name": "YosemitechModbus",
            "version": "https://github.com/EnviroDIY/YosemitechModbus.git"
        }
        {
            "library id": "873",
            "name": "RTCZero",
            "url": "https://github.com/arduino-libraries/RTCZero.git"
        },
    ]
}
